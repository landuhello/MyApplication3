<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#141931"
    tools:context=".view.activity.Main9Activity">
<RelativeLayout
    android:layout_marginTop="37dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >
    <ImageView
        android:id="@+id/fanhui"
        android:layout_marginLeft="24dp"
        android:src="@drawable/shapee"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView

        android:layout_centerHorizontal="true"
        android:textSize="16sp"
        android:text="我的"
        android:textColor="#ffffff"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"></TextView>

</RelativeLayout>
    <LinearLayout
        android:background="#1c2243"
        android:layout_gravity="center"
        android:layout_marginTop="80dp"
        android:layout_width="312dp"
        android:layout_height="178dp"
        android:orientation="vertical"
        >
        <LinearLayout
            android:layout_marginTop="20dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <TextView
                android:layout_gravity="center"
                android:layout_marginLeft="20dp"
                android:layout_width="57dp"
                android:layout_height="14dp"
                android:textSize="14sp"
                android:textColor="#deffffff"
                android:includeFontPadding="false"
                android:text="头&#12288;&#12288;像"
                />
            <ImageView
                android:id="@+id/my_tou"
                android:layout_marginLeft="174dp"
                android:layout_width="36dp"
                android:layout_height="36dp"
                ></ImageView>
        </LinearLayout>
        <LinearLayout
            android:layout_marginTop="15dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <TextView
                android:includeFontPadding="false"
                android:layout_gravity="center"
                android:layout_marginLeft="20dp"
                android:layout_width="57dp"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:textColor="#deffffff"
                android:text="昵&#12288;&#12288;称"
                />
           <TextView
               android:id="@+id/my_name"
               android:layout_marginLeft="174dp"
               android:textColor="#deaaaaaa"
               android:text="电影故事"
               android:textSize="12sp"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"></TextView>
        </LinearLayout>
        <LinearLayout
            android:layout_marginTop="15dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <TextView
                android:includeFontPadding="false"
                android:layout_gravity="center"
                android:layout_marginLeft="20dp"
                android:layout_width="57dp"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:textColor="#deffffff"
                android:text="性&#12288;&#12288;别"
                />
            <TextView
                android:includeFontPadding="false"
                android:id="@+id/my_sex"
                android:layout_marginLeft="174dp"
                android:textColor="#deaaaaaa"
                android:text="电影故事"
                android:textSize="12sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
        </LinearLayout>
        <RelativeLayout
            android:layout_marginTop="15dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >
            <TextView
                android:layout_gravity="center"
                android:layout_marginLeft="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:textColor="#deffffff"
                android:text="出生日期"
                />
            <TextView
                android:id="@+id/my_time"
                android:layout_alignParentRight="true"
                android:textColor="#deaaaaaa"
                android:text="2019-12-25"
                android:textSize="12sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
        </RelativeLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_gravity="center"
        android:layout_width="312dp"
        android:layout_height="57dp"
        android:orientation="vertical"
        android:background="#1c2243"
        android:id="@+id/li11" 
        android:layout_marginTop="10dp"
        >
        <LinearLayout
            android:layout_marginTop="8dp"
            android:layout_width="312dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <TextView
            android:layout_width="57dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="20dp"
            android:text="手  机  号"
            android:textColor="#deffffff"
            android:textSize="14sp" />

            <TextView

            android:id="@+id/my_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="104dp"
            android:text="139-94073-577"
            android:textColor="#deaaaaaa"
            android:textSize="12sp"></TextView>

        </LinearLayout>
        <LinearLayout
            android:layout_marginTop="8dp"
            android:layout_width="312dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <TextView
                android:layout_width="57dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="20dp"
                android:text="邮&#12288;&#12288;箱"
                android:textColor="#deffffff"
                android:textSize="14sp" />

            <TextView

                android:id="@+id/my_email"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="104dp"
                android:text="25464516@qq.com"
                android:textColor="#deaaaaaa"
                android:textSize="12sp"></TextView>


    </LinearLayout>
</LinearLayout>
</LinearLayout>